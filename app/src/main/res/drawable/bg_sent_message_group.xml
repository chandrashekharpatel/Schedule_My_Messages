            <corners android:bottomRightRadius="0dp"
                android:bottomLeftRadius="3dp"
                android:topRightRadius="3dp"
                android:topLeftRadius="3dp"/>
            <solid android:color="#1befefef" />
        </shape>
    </item>
</layer-list>